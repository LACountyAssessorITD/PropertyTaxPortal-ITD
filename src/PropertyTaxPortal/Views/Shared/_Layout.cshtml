<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] PopertyTaxPortal</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link href="~/css/site.css" rel="stylesheet" />
        <link href="~/css/queires.css" rel="stylesheet" />
        <link href="~/css/footer.css" rel="stylesheet" />
        <link href="~/css/nav.css" rel="stylesheet" />
        <link href="~/css/button.css" rel="stylesheet" />
        <link href="https://fonts.googleapis.com/css?family=Lato:100,300,300i,400" rel="stylesheet">
        <link href="https://unpkg.com/ionicons@4.2.2/dist/css/ionicons.min.css" rel="stylesheet">
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.1/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
    </environment>
    <environment exclude="Development">
        <link href="~/css/button.css" rel="stylesheet" asp-append-version="true" />

        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css"
              integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS"
              crossorigin="anonymous" asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/footer.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/nav.css" asp-append-version="true" />
        <link rel="sytlesheet" href="~/css/queires.css" asp-append-version="true" />
        <link href="https://fonts.googleapis.com/css?family=Lato:100,300,300i,400" rel="stylesheet" asp-append-version="true" />
        <link href="https://unpkg.com/ionicons@4.2.2/dist/css/ionicons.min.css" rel="stylesheet" asp-append-version="true" />
    </environment>

</head>
<body class="">

    <!------------------------------ NAVBAR ------------------------------>
    <nav class="navbar navbar-expand-lg bg-light">
        <a class="navbar-brand" href="#"><img src="~/images/LACseal76.png" alt=""></a>
        <div class="nav-icon-bar">
            <a href="">
                <p>Los Angeles County</p>
            </a>
            <a href="">
                <h5>Property Tax Portal</h5>
            </a>
        </div>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <ion-icon class="icon" name="menu"></ion-icon>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav ">
                <li class="nav-item ml-auto">
                    <a class="nav-link" href="#">Home</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">
                        Overview
                    </a>
                    <div class="dropdown-menu small-drop" aria-labelledby="navbarDropdown">
                        <ul>
                            <li>Property Tax Overview</li>
                            <li>How the Property Tax System Works</li>
                            <li>Areas of Responsibliity</li>
                        </ul>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">
                        Tax Tools
                    </a>
                    <div class="dropdown-menu small-drop" aria-labelledby="navbarDropdown">
                        <ul>
                            <li>Property Tax Managment System</li>
                            <li>Multiple Parcel Inquires</li>
                            <li>Avoid Penalties by Understanding Postmarks</li>
                            <li>Penalty Cancellations</li>
                            <li>Property Address Search</li>
                            <li>New Home Owners</li>
                            <li>New Business Owners</li>
                        </ul>
                    </div>
                </li>
                <li class="nav-item dropdown-left">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">
                        Quick Links
                    </a>
                    <div class="dropdown-menu mega-menu" aria-labelledby="navbarDropdown">
                        <div class="row">
                            <div class="col-lg-3 col-md-6">
                                <p>Forms</p>
                                <ul>
                                    <li>Assessor</li>
                                    <li>Assessment Appeals Board</li>
                                    <li>Auditor-Controller</li>
                                    <li>Treasurer & Tax Collector</li>
                                </ul>
                            </div>
                            <div class="col-lg-3 col-md-6">
                                <p>Property Valuation</p>
                                <ul>
                                    <li>Contesting Your Property Value</li>
                                    <li>Property Address Search</li>
                                    <li>Property Data & Maps</li>
                                    <li>Supplemental Tax Estimator</li>
                                </ul>
                            </div>
                            <div class="col-lg-3 col-md-6">
                                <p>Payment Options</p>
                                <ul>
                                    <li>How Much Do I Owe?</li>
                                    <li>Pay Online</li>
                                    <li>Other Payment Options</li>
                                </ul>
                            </div>
                            <div class="col-lg-3 col-md-6">
                                <p>Annual Secured Property Tax Bill</p>
                                <ul>
                                    <li>Annual Secured Property Tax Bill Insert</li>
                                    <li>Annual Secured Property Tax Information Statement</li>
                                    <li>Notice of Delinquency</li>
                                    <li>Substitute Secured Property Tax Bill</li>
                                    <li>Supplemental Secured Property Tax Bill</li>
                                    <li>Unsecured Property Tax Bill</li>
                                    <li>Unsecured Prior Year Bill</li>
                                    <li>Payment Activity Notice</li>
                                    <li>Payment Activity Notice</li>
                                    <li>Statement of Prior Year Taxes</li>
                                    <li>Adjusted Annual Property Tax Bill</li>
                                    <li>Adjusted Supplemental Property Tax Bill</li>
                                </ul>
                            </div>
                        </div>

                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">
                        FAQ
                    </a>
                    <div class="dropdown-menu small-drop" aria-labelledby="navbarDropdown">
                        <ul>
                            <li>All FAQs</li>
                            <li>Tax Bills</li>
                            <li>Refunds/Tax Sales/Tax Liens</li>
                            <li>Property Values</li>
                            <li>Ownership</li>
                        </ul>
                    </div>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
                       aria-haspopup="true" aria-expanded="false">
                        Contact Us
                    </a>
                    <div class="dropdown-menu small-drop" aria-labelledby="navbarDropdown">
                        <ul>
                            <li>Assessor</li>
                            <li>Auditor/Controller</li>
                            <li>Treasurer & Tax Collector</li>
                            <li>Assessment Appeals Board</li>
                            <li>LA County Contacts</li>
                            <li>Public Inquiry</li>
                        </ul>
                    </div>
                </li>
                <li>
                    <ion-icon class="search-font" name="search"></ion-icon>
                </li>
            </ul>
        </div>

    </nav>


    <!----------------------------- /END NAVBAR -------------------------->

    <div class="container-fluid ">
        <!------------------------------ BODY ---------------------------->
        @RenderBody()

        <!----------------------------- /END BODY ------------------------->


    </div>
    <!------------------------------ FOOTER ------------------------------>
    
    <nav class="navbar navbar-dark  px-0">
        <div class="footer container-fluid">
            <div class="row  pt-5">
                <div class="col-2">
                    <center>
                        <h1><img src="~/images/LACseal.png" /></h1>
                        <ul>
                            <list>Enriching Lives . . . </list>
                        </ul>
                    </center>
                </div>
                <div class="col-2">
                    <h2 class="display-6">Do You Need Help?</h2>
                    <ul>
                        <li>211 LA County</li>
                        <li>LA County Helps</li>
                        <li>Public Alerts</li>
                    </ul>
                </div>
                <div class="col-2">
                    <h2 class="display-6">About</h2>
                    <ul>
                        <li>Navigation</li>
                        <li>How to Use</li>
                        <li>Site Map</li>
                    </ul>
                </div>
                <div class="col-2">
                    <h2 class="display-6">Site Info</h2>
                    <ul>
                        <li>Accessibility</li>
                        <li>Disclaimer</li>
                        <li>Language</li>
                        <li>Privacy Policy</li>
                    </ul>
                </div>
                <div class="col-2">
                    <h2 class="display-6">Contact</h2>
                    <ul>
                        <li><a asp-area="" asp-controller="Home" asp-action="PublicInquiry">Public Inquiry Form</a></li>
                        <li>Assessor</li>
                        <li>Auditor/Controller</li>
                        <li>Treasurer & Tax Collector</li>
                        <li>Assessment Appeals Board</li>
                        <li>LA County Contacts</li>
                    </ul>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </nav>

    <footer class="">
        <nav class="navbar navbar-brand  fixed-bottom color-4 mx-0">

            <span class="navbar-brand">&copy; @DateTime.Now.Year - Property Tax Portal</span>

        </nav>

    </footer>
    <!-- Footer -->
    




    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo">
        </script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                integrity="sha384-B0UglyR+jN6CkvvICOB2joaf5I4l3gm9GU6Hc1og6Ls7i6U/mkkaduKaBhlAXv9k"
                crossorigin="anonymous">
        </script>
        <script src="~/lib/jquery-validation/dist/jquery.validate.js" asp-append-version="true"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js" asp-append-version="true"></script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>


</html>
